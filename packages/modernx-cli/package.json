{
  "name": "modernx-cli",
  "version": "1.3.0",
  "description": "The modernx command line utility.",
  "repository": {
    "type": "git",
    "url": "https://github.com/perlinson/modernx.git"
  },
  "homepage": "https://github.com/perlinson/modernx#readme",
  "author": "perlinson <perlinson2024@gmail.com>",
  "license": "MIT",
  "bin": {
    "modernx": "./bin/modernx"
  },
  "scripts": {
    "build": "rm -rf lib && npx babel src --out-dir lib",
    "dev": "npx babel src --out-dir lib --watch",
    "test": "node test/cli.test.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.8",
    "inquirer": "^9.1.4",
    "ora": "^6.3.1",
    "path-exists": "^5.0.0",
    "semver": "^7.5.4",
    "through2": "^4.0.2",
    "update-notifier": "^6.0.2",
    "vinyl-fs": "^3.0.3",
    "which": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "babel-plugin-add-module-exports": "^1.0.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "14"
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "files": [
    "lib",
    "src",
    "templates",
    "bin"
  ],
  "keywords": [
    "modernx",
    "react",
    "react18",
    "cli",
    "generator",
    "scaffold",
    "state-management"
  ],
  "gitHead": "f14cf7c9309547fc2d9013ac8baa5495e2641750"
}
